<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:gravity="center_horizontal"
android:orientation="vertical"
android:paddingLeft="@dimen/activity_horizontal_margin"
android:paddingTop="@dimen/activity_vertical_margin"
android:paddingRight="@dimen/activity_horizontal_margin"
android:paddingBottom="@dimen/activity_vertical_margin"
android:background="#123456"
tools:context=".adminLanding">


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="127dp"
        android:layout_marginTop="50dp"
        app:srcCompat="@drawable/logo"/>

    <!-- Login progress -->
    <ProgressBar
        android:id="@+id/login_progress"
        style="?android:attr/progressBarStyleLarge"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:visibility="gone" />


        <Button
            android:id="@+id/btnaddtrainer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Add Trainer" />

        <Button
            android:id="@+id/btnremovetrainer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Remove Trainer" />

        <Button
            android:id="@+id/btnapprovecustomer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Approve Customer" />

        <Button
            android:id="@+id/btnassigntrainer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Assign Trainer" />


        <Button
            android:id="@+id/btnaddpackage"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Add Package" />

        <Button
            android:id="@+id/btnhealthforum"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Health Forum" />
        <Button
            android:id="@+id/btnlogout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Logout" />


</LinearLayout>
